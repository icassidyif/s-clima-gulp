section.products
  .row

    .column.column-sm-1-2.column-md-1-3
      .products__card.card-product
        .card-product__slider.slider-card-product.popup-gallery
          .slider-card-product__item
            a(href='img/products/1.png')
              +image('products/1.png')
          .slider-card-product__item
            a(href='img/products/1.png')
              +image('products/1.png')
        .card-product__container
          h6.card-product__title Кондиціонер спліт Vestfrost VFE07AF5 inverter (Wi-Fi опція)
          .card-product__inline
            span.card-product__price 5999 грн
            .card-product__cart
              +icon('cart-product')
          .card-product__inline
            span.card-product__label Код товару:
            span.card-product__value 568-92
          .card-product__inline
            span.card-product__label Тип:
            span.card-product__value Спліт-система
          .card-product__inline
            span.card-product__label Компресор:
            span.card-product__value Інверторний
          //more block-----------------------------
          .card-product__rest
            .card-product__wrapper
              .card-product__inline
                span.card-product__label Діапазон температур, С:
                span.card-product__value -7, +43
              .card-product__inline
                span.card-product__label Холодопродуктивність, кВт:
                span.card-product__value 2,21
              .card-product__inline
                span.card-product__label Теплопродуктивність, кВт:
                span.card-product__value 3,21
              .card-product__inline
                span.card-product__label Електроспоживання, кВт
              .card-product__inline.card-product__inline_sub
                span.card-product__label - охолодження:
                span.card-product__value 3,21
              .card-product__inline.card-product__inline_sub
                span.card-product__label - обігрів:
                span.card-product__value 5,21
              .card-product__inline
                span.card-product__label Країна виробник:
                span.card-product__value Китай

              a(href='#').card-product__link.link Детальніше

        .card-product__more.opened
          span





